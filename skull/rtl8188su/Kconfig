config RTLWIFI
	tristate
	select FW_LOADER

config RTLWIFI_PCI
	tristate

config RTLWIFI_USB
	tristate

config RTLWIFI_DEBUG
	bool "Debugging output for rtlwifi driver family"
	depends on RTLWIFI
	default m
	---help---
	To use the module option that sets the dynamic-debugging level for,
	the front-end driver, this parameter must be "Y". For memory-limited
	systems, choose "N". If in doubt, choose "Y".

config RTLWIFI_DEBUGFS
	bool "Debugfs Support"
	depends on RTLWIFI_DEBUG
	depends on DEBUG_FS
	default n
	help
	 Export several driver and device internals to user space
	 via debugfs. Be be advised: writing random data in
	 "hw_iowrite" might damage the device.

	 Say N, unless you have a good reason.

config RTL8192C_COMMON
	tristate
	depends on RTL8192CE || RTL8192CU
	default m

config RTL8723_COMMON
	tristate
	depends on RTL8723AE || RTL8723BE
	default m

config RTLBTCOEXIST
	tristate
	depends on RTL8723AE || RTL8723BE || RTL8821AE || RTL8192EE
	default m
